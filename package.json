{
  "name": "nativescript-vue-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clear": "rimraf dist",
    "build": "npm run lint && npm run clear && babel src -d dist",
    "lint": "eslint 'src/**/*.{js,vue}' --fix",
    "pretest": "npm run lint",
    "test": "npm run test:unit",
    "test:unit": "vue-cli-service test:unit",
    "preversion": "npm test && npm run build",
    "version": "git add -A lib",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-vue-app": "^2.0.0",
    "chai": "^4.1.2",
    "eslint": "^5.0.1",
    "eslint-plugin-vue": "^4.5.0",
    "lint-staged": "^6.0.0",
    "mocha": "^5.2.0",
    "node-sass": "^4.9.0",
    "rimraf": "^2.6.2"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "npm run lint",
      "git add"
    ],
    "*.vue": [
      "npm run lint",
      "git add"
    ]
  }
}
